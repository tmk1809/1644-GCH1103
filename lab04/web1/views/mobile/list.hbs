<!-- sử dụng emmet code để tạo bảng nhanh -->
{{!-- table>(tr>(th*3))+(tr>(td*3)) --}}
<table border="1">
   <tr>
      <th>Number</th>
      <th>Mobile</th>
      <th>Image</th>
   </tr>
   {{#each mobiles }}
   <tr>
      <td>{{ number }}</td>
      <td>{{ mobile }}</td>
      <td><img src="{{ image }}" width="100" height="100"></td>
   </tr>
   {{/each}}
</table>